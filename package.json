{
  "name": "react-express-heroku-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\""
  },
  "dependencies": {
    "clean-css": "^4.2.3",
    "constantinople": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "helmet": "3.21.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "node-fetch": "2.6.0",
    "request": "2.88.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "concurrently": "4.1.2",
    "mocha": "6.2.0"
  }
}
